<Window x:Class="Snake.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Snake"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"  KeyDown="Canvas_KeyDown">

    <Canvas Margin="15" Background="Black" >

        <Rectangle Fill="White" Width="50" Height="50" RadiusX="15" RadiusY="15" Name="Rect" Canvas.Left="360" Canvas.Top="177" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Canvas>
</Window>
